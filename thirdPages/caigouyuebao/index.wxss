.divider{
  width: 100%;
  height: 24rpx;
  background: #F7F8FA;

}

.price-box-list{
  display: flex;
  flex-direction: row;
}
.price-box-item{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.price-box-item>text{
  font-size: 36rpx;
font-weight: 600;
color: #333333;
}
.price-box-item>view{
display: flex;
flex-direction: row;
align-items: center;
margin-top: 4rpx;

}
.price-box-item>view>image{
  width: 31rpx;
  height: 31rpx;
}
.price-box-item>view>text{
  font-size: 24rpx;
font-weight: 400;
color: #333333;
margin-left: 2rpx;
}
