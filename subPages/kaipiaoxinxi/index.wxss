/* subPages/shezhi/index.wxss */
page{
    background: #F7F8FA;
  }
  .background {
    width: 100%;
    height: 160rpx;
    background: #2968ed;
    border-radius: 0 0 32rpx 32rpx;
position: absolute;
top: 0;
left: 0;
z-index: -1;
  
  }
  .commen-box{
    width: 94%;
    margin: 20rpx auto 0;
    /* padding: 0 24rpx ; */
    box-sizing: border-box;
    background-color: #FFFFFF;
    padding-bottom: 1rpx;
  }

  .commen-box-title{
      width: 100%;
      height: 90rpx;
      background: linear-gradient(180deg, #CEDDFF 0%, #FFFFFF 100%);
      display: flex;
      flex-direction: row;
      align-items: center;
  }
  .commen-box-title>view{
      width: 8rpx;
      height: 32rpx;
      background: #6191FD;
      margin-left: 24rpx;
  }
  .commen-box-title>text{
    font-size: 30rpx;
    font-weight: 500;
    color: #333333;
    margin-left: 12rpx;
  }
  .commen-view-item{
    display: flex;
    flex-direction: row;
    margin-bottom: 40rpx;
    padding: 0 24rpx;
  
  }
  .text1{
    font-size: 28rpx;
font-weight: 400;
color: #999999;
width: 112rpx;
  }
  .text2{
    font-size: 28rpx;
font-weight: 400;
color: #333333;
margin-left: 48rpx;
  }
  .text3{
    flex: 1;
  }
  .yrz{
    width: 104rpx;
    height: 32rpx;
  }
  .right{
    width: 40rpx;
    height: 40rpx;
  }

  .confirm{
      width: 94%;
      margin: 48rpx auto;
      height: 88rpx;
background: linear-gradient(312deg, #2362ED 0%, #6191FD 100%);
border-radius: 44rpx ;
color: #FFFFFF;
font-size: 34rpx;
text-align: center;
line-height: 88rpx;
  }