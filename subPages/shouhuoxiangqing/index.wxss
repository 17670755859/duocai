font-weight: 400;
color: #999999;
min-width: 120rpx;
}
.order-item>text:nth-child(2){
  font-size: 30rpx;
font-weight: 400;
color: #333333;
flex: 1;
margin-left: 40rpx;
}
.pay-mode{
