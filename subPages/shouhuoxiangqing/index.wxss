page {
  background: #F7F8FA;
}

.container {
  padding-top: 0;
}

.commen-view {
  width: 100%;
  background: #ffffff;
  border-radius: 24rpx;
  box-sizing: border-box;
  padding-bottom: 32rpx;
  margin-top: 24rpx;
  padding-top: 24rpx;
}

.commen-view>.shxx {
  font-size: 30rpx;
  font-weight: 600;
  color: #333333;
  /* padding-top: 32rpx; */
  padding: 8rpx 32rpx 32rpx;
}

.commen-view>.line {
  height: 6rpx;
  width: 98%;
  margin: 0 auto;
  display: block;
}

.commen-view>.name {
  font-size: 32rpx;
  font-weight: 600;
  color: #333333;
  padding: 48rpx 32rpx 16rpx;
}

.commen-view>.address {
  font-size: 26rpx;
  font-weight: 400;
  color: #333333;
  padding: 0 32rpx;
}

.tags {
  display: flex;
  flex-direction: row;
  align-items: center;
  /* justify-content: center; */
  height: 56rpx;

  border-radius: 0 28rpx 28rpx 0;
  width: 155rpx;
  /* margin-top: 24rpx; */
}

.dc {
  background: linear-gradient(270deg, #2362ED 0%, #6191FD 100%);
}

.yx {

  background: #FF6200;
}

.jd {
  background: linear-gradient(118deg, #FF8EA4 0%, #FF6C75 54%, #FF504E 100%);
}

.tags>image {
  width: 48rpx;
  height: 48rpx;
}

.tags>text {
  font-size: 30rpx;
  font-weight: 500;
  color: #FFFFFF;
  margin-left: 6rpx;
}

.image-list {
  margin-top: 32rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.image-item {
  width: 124rpx;
  height: 124rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  margin-left: 24rpx;
}

.total-image {
  width: 124rpx;
  height: 124rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  background: #F7F8FA;
  /* box-shadow: -2rpx 0 0 1rpx #EEEEEE; */
  margin-left: 24rpx;
}

.other-tips {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;

  margin-top: 30rpx;
}

.dcjs {
  background: #FFF4ED;
  padding: 6rpx 0;
}

.other-tips-text1 {
  font-size: 30rpx;
  font-weight: 400;
  color: #333333;
}

.other-tips-text2 {
  font-size: 30rpx;
  font-weight: 500;
  color: #333333;
  margin-left: 8rpx;
  margin-right: 18rpx;
}

.other-tips-text3 {
  flex: 1;
  text-align: left;
  margin-left: 24rpx;
}

.dcjs-text {
  color: #FF6200;
}

.other-price {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 36rpx;
}

.other-price>image {
  width: 31rpx;
  height: 31rpx;
  margin-left: 24rpx;
}

.other-price-title {
  margin-left: 16rpx;
  font-size: 30rpx;
  font-weight: 400;
  color: #999999;
  flex: 1;
}

.other-price-info {
  font-size: 30rpx;
  font-weight: 400;
  color: #999999;
  margin-right: 24rpx;
}

.divider {
  width: 100%;
  height: 8rpx;
  background: #F7F8FA;
  margin: 24rpx 0;
}

.dis-mode-item {
  display: flex;
