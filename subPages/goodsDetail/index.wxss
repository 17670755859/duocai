.container{
  padding-bottom: 200rpx;
}
.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;

  z-index: 9999999;

}

.nav-back {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background: rgba(255, 255, 255, 0.6);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-left: 24rpx;
}

.nav-back>image {
  width: 40rpx;
  height: 40rpx;
}

.nav-name {
  font-size: 36rpx;
  font-weight: 500;
  color: #333333;
  flex: 1;
  text-align: center;
}

.nav-title {

  margin: auto;
  font-size: 32rpx;
  color: #fff;

  display: flex;
  flex-direction: row;
  align-items: center;
  /* width: 100%; */
}


.nav-title .name {
  font-size: 30rpx;
  color: #FFFFFF;
  margin-left: 20rpx;

}
.swiper-box{
  width: 100%;
  height: 750rpx;
}
.goods-image {
  width: 100%;
  height: 750rpx;
  background-position: center;
  background-size: 100%;
  background-repeat: no-repeat;
}

.goods-detail {
  width: 94%;
  margin: 20rpx auto;
}

.price {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.price>.cur-unit {
  color: #FF6200;
  font-size: 32rpx;
}

.price>.cur-price {
  color: #FF6200;
  font-size: 40rpx;
}

.price>.ori-price {
  color: #999999;
  font-size: 26rpx;
  margin-left: 24rpx;
  flex: 1;
}

.goods-title-detail{
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap: wrap;
  margin-top: 46rpx;
}

.goods-title-detail>view {
  /* height: 32rpx; */
  border-radius: 16rpx;
  border: 1px solid #2362ED;
  color: #2362ED;
  color: 24rpx;
  padding: 0 12rpx;
  font-size: 24rpx;
}

.goods-title-detail>text {
  flex: 1;
  margin-left: 16rpx;
  font-size: 32rpx;
  font-weight: 600;
  color: #333333;
}

.content {
  font-size: 32rpx;
  font-weight: 600;
  color: #333333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  min-width: 0;
  margin-top: 16rpx;
}

.collect {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
  margin-top: 34rpx;
}

.collect>image {
  width: 40rpx;
  height: 40rpx;
}

.collect>text {
  font-size: 26rpx;
  color: #333333;
  margin-left: 10rpx;
}

.divider {
  width: 100%;
  height: 24rpx;
  background: #F7F8FA;
}

.price-box-title {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.price-box-title>view {
  width: 8rpx;
  height: 32rpx;
  background: #6191FD;
}

.price-box-title>text {
  font-size: 30rpx;
  font-weight: 500;
  color: #333333;
  margin-left: 12rpx;
}

.price-list {
  margin-top: 40rpx;
}

.price-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 30rpx;
}

.price-item>image {
  width: 31rpx;
  height: 31rpx;
}

.price-item-title {
  margin-left: 16rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
  flex: 1;
}

.price-item-content {
  font-size: 28rpx;
  color: #999999;
}

.price-item-title-1 {
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
  flex: 1;
}

.price-item-content-1 {
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}

.space {
  background: #F7F8FA;
  padding: 24rpx 0;
  margin-top: 40rpx;
}

.space-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 94%;
  margin: 24rpx auto 0;
}

.space-item>text {
  font-size: 28rpx;
}

.space-item>text:nth-child(1) {
  color: #999999;
  width: 150rpx;
  /* border: 1px solid red; */
}

.space-item>text:nth-child(2) {
  color: #333333;
  flex: 1;
  margin-left: 100rpx;
}

.add-car-box {
  display: flex;
  flex-direction: row;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 96rpx;
  align-items: center;
  z-index: 9999;
}

.add-car-box-text1 {
  color: #333333;
  font-size: 24rpx;
  margin-left: 24rpx;

}

.add-car-box-text2 {
  font-size: 24rpx;
  color: #FF6200;
}

.add-car-box-text3 {
  font-size: 34rpx;
  color: #FF6200;
  flex: 1;
}

.add-car-box-btn1 {
  width: 192rpx;
  height: 72rpx;
  background: #FB944C;
  border-radius: 44rpx;
  text-align: center;
  line-height: 72rpx;
  color: #fff;
  font-size: 28rpx;
}

.add-car-box-btn2 {
  width: 192rpx;
  height: 72rpx;
  background: linear-gradient(312deg, #2362ED 0%, #6191FD 100%);
  border-radius: 44rpx;
  text-align: center;
  line-height: 72rpx;
  color: #fff;
  font-size: 28rpx;
  margin-left: 8rpx;
}
.car-box{
  bottom: 240rpx;
  right: 24rpx;
  position: fixed;
  z-index: 1000;
}
.car{
  width: 102rpx;
  height: 102rpx;

}
.car-num{
  /* border: 1px solid #FF6868; */
  /* color: #FF6868; */
  position: fixed;
  bottom: 306rpx;
  right: 34rpx;
  font-size: 18rpx;
  border-radius: 12rpx;
  background:#FF6868 ;
  color: #fff;
  padding: 0 4rpx;
}


.wx-swiper-dot{
  width: 10rpx;
  height: 10rpx;
  background: #FFFFFF !important;
  display: inline-flex;
  margin-left: 5rpx;  /*两个点的距离*/
  justify-content:space-between;
  border-radius: 50%;
 }
 .wx-swiper-dot::before{
  content: '';
  flex-grow: 1; 
  background : #FFFFFF; /*点的未选中颜色*/
  border-radius: 0rpx;
  
 }
 .wx-swiper-dot-active::before{
  background : #FFFFFF;  /*点的选中颜色*/
 }
  
.wx-swiper-dot.wx-swiper-dot{ /*点的未选中样式*/
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
 }
 .wx-swiper-dot.wx-swiper-dot-active{/*点的选中样式*/
  width: 33rpx;
  height: 10rpx;
  background: #FFFFFF;
  border-radius: 6rpx ;
 }