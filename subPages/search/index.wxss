page{
  background: #F7F8FA;
}
.search{
  width: 100%;
  background: #fff;
  padding-bottom: 40rpx;
}
.search-btn{
  width: 120rpx;
height: 72rpx;
background: linear-gradient(312deg, #2362ED 0%, #6191FD 100%);
border-radius: 44rpx;
text-align: center;
line-height: 72rpx;
color: #fff;font-size: 28rpx;
}
.del-box{
  width: 94%;
  margin: 20rpx auto;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.del-box>text{
  font-size: 25rpx;
font-weight: 400;
color: #333333;
}
.del-box>image{
  width: 32rpx;
  height: 32rpx;
  margin-left: 16rpx;
}
.list{
  display: flex;
  flex-direction: row;
  align-items: center;
  /* justify-content: space-around; */
  flex-wrap: wrap;
}
.list>view{
  height: 64rpx;
background: #F3F3F3;
border-radius: 32rpx ;
text-align: center;
line-height: 64rpx;
padding: 0 32rpx;
margin-left: 16rpx;
margin-bottom: 32rpx;
color: #333333;
font-size: 28rpx;
}