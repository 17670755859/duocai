@import '../common/index.wxss';

.van-tabbar {
  background-color: var(--tabbar-background-color, #fff);
  box-sizing: initial;
  display: flex;
  /* height: var(--tabbar-height, 50px); */
  width: 100%;
  /* height: 136rpx; */
}

.van-tabbar--fixed {
  bottom: 0;
  left: 0;
  position: fixed;
  z-index: 100;
}

.van-tabbar--safe {
  padding-bottom: env(safe-area-inset-bottom)
}